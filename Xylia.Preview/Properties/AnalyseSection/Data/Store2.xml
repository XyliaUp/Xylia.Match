    <!-- 基本信息 -->
    <record alias="alias" type="Index" />
    <record alias="name2" name="商店名" server="n" type="TextAlias" />
    <record alias="icon" name="商店前景图片" server="n" type="Icon" />
    <record alias="none-selected-icon" name="商店后景图片" server="n" type="Icon" />
    <record alias="faction" ref="Faction" name="" />
    <!-- item- -->
    <record alias="item" ref="Item" repeat="127" />
    <record alias="buy-price" ref="ItemBuyPrice" repeat="127" />
  </list>
</table>